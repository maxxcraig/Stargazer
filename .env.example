# Environment Configuration Template
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# NASA API Configuration (optional - for real-time astronomical data)
NASA_API_KEY=your_nasa_api_key_here

# App Configuration
APP_ENV=development
DEBUG_MODE=true

# Security Configuration
ENCRYPTION_KEY=your_256_bit_encryption_key_here
API_TIMEOUT=10000

# Star Catalog Configuration
STAR_CATALOG_VERSION=1.0.0
MAX_VISIBLE_STARS=2000

# Location Services
LOCATION_ACCURACY=high
LOCATION_TIMEOUT=15000

# Development Settings
ENABLE_DEBUG_OVERLAY=true
LOG_LEVEL=info